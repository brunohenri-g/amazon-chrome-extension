{
	"name": "Amazon Whishlist Organizer",
	"description": "Aprimora os filtros da tela de Wishlist da Amazon",
	"version": "1.0",
	"manifest_version": 3,
	"options_page": "options.html",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"storage",
		"activeTab",
		"scripting"
	],
	"action": {
		"default_popup": "popup.html"
	}
}